{
  "name": "exampleNuxtCSSModuleTailwind",
  "version": "1.0.0",
  "description": "Example to use CSSModule Nuxt and Tailwind",
  "author": "Aleksandr Zidyganov",
  "private": true,
  "project": {
    "baseURL": "/NuxtCSSModuleTailwind"
  },
  "scripts": {
    "dev": "nuxt --verbose",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "tailwind:cssBase": "npx tailwind build assets/css/tailwind-base.src.css -o assets/css/tailwind-base.css",
    "tailwind:css": "npx tailwind build assets/css/tailwind.src.css -o assets/scss/_tailwind.scss"
  },
  "dependencies": {
    "nuxt": "2.11.0",
    "tailwindcss": "1.1.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "1.1.2",
    "@nuxtjs/eslint-module": "1.0.0",
    "@nuxtjs/style-resources": "1.0.0",
    "@nuxtjs/tailwindcss": "1.3.1",
    "babel-eslint": "10.0.3",
    "css-mqpacker": "7.0.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-nuxt": "0.4.3",
    "eslint-plugin-prettier": "3.1.2",
    "node-sass": "4.13.1",
    "nuxt-purgecss": "0.2.1",
    "prettier": "1.16.4",
    "sass-loader": "8.0.2",
    "stylelint": "13.0.0",
    "stylelint-config-recommended-scss": "4.1.0",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.13.0"
  }
}
